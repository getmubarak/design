package problem9;

interface Account{
	Dialog createUI();
	...
}
class SA implements Account {
	Dialog createUI(){
		return new SADialog();
	}
	...
}
class CA implements Account {
	Dialog createUI(){
		return new CADialog();
	}
	...
}
public class Entry {
	static void main()
	{
		Account account = new SA();
		Dialog dlg=account.createUI(); //<-- factory method pattern
		dlg.Display();
		
	}
}
